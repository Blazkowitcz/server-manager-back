{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "single"],
        "eqeqeq": ["error", "always"],
        "object-curly-spacing": "off",
        "space-before-function-paren": "off",
        "eol-last": "off"
    }
}
